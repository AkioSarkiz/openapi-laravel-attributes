{
    "name": "akiosarkiz/openapi-laravel-attributes",
    "type": "library",
    "description": "Openapi laravel attributes.",
    "keywords": ["openapi", "laravel", "attributes"],
    "homepage": "https://github.com/akiosarkiz/openapi-laravel-attributes",
    "license": "MIT",
    "authors": [
        {
            "name": "AkioSarkiz",
            "email": "akiosarkiz@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^3.2",
        "jetbrains/phpstorm-attributes": "^1.0",
        "orchestra/testbench": "^6.19",
        "spatie/enum": "^3.11",
        "symfony/finder": "^5.3",
        "symfony/http-foundation": "^5.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "jeroen-g/laravel-packager": "^2.7",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.28",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "AkioSarkiz\\Openapi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AkioSarkiz\\Openapi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "AkioSarkiz\\Openapi\\OpenapiAttributesServiceProvider"
            ]
        }
    }
}
